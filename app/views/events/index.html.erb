<ul>
<% @events.each do |event| %>
  <li>
 	  <%= event.name %>
  <%= link_to 'Show', event_path(event) %>
  <%= link_to 'Edit', edit_event_path(event) %>
  <%= button_to 'delete', event_path(event), :method => :delete %>
  </li>
<% end %>
</ul>
<%= link_to 'New event', :controller => 'events', :action => 'new' %>
<%= paginate @events %>